             name="%category.name.1">
          <tool
                name="%tool.name.10"
                object="SequenceFlow">
          </tool>
       </category>
       <category
             id="org.eclipse.bpmn2.modeler.runtime.marklogic.workflow.toolpalette.cpf.Events"
             name="%category.name.2">
          <tool
                name="%tool.name.93"
                object="CPFInitial">
