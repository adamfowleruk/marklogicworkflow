             name="%category.name.3">
          <tool
                name="%tool.name.91"
                object="CPFCondition">
          </tool>
       </category>
       <category
             id="org.eclipse.bpmn2.modeler.runtime.marklogic.workflow.toolpalette.cpf.Artifacts"
             name="%category.name.10">
          <tool
                name="%tool.name.39"
                object="Group">
          </tool>
          <tool
                name="%tool.name.40"
                object="TextAnnotation">
          </tool>
       </category>
       <category
             description="MarkLogic Content Processing Framework Activities"
             id="org.eclipse.bpmn2.modeler.runtime.marklogic.workflow.toolpalette.cpf.CPF"
             name="%category.name.22">
          <tool
                name="%tool.name.95"
                object="CPFStatus">
          </tool>
          <tool
                name="%tool.name.90"
                object="CPFState">
          </tool>
          <tool
                name="%tool.name.92"
                object="CPFAction">
          </tool>
       </category>
    </toolPalette>
